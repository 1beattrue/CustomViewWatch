<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomWatch">
        <attr name="shape" format="enum">
            <enum name="round" value="0" />
            <enum name="square" value="1" />
        </attr>
        <attr name="hasNumbers" format="boolean" />
        <attr name="hasSeconds" format="boolean" />
        <attr name="dialColor" format="color" />
        <attr name="mainColor" format="color" />
        <attr name="secondHandColor" format="color" />
    </declare-styleable>
</resources>